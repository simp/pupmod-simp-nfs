{
  "name": "simp-nfs",
  "version": "7.2.0",
  "author": "SIMP Team",
  "summary": "manages NFS server and client, also PKI and stunnelling",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-nfs",
  "project_page": "https://github.com/simp/pupmod-simp-nfs",
  "issues_url": "https://simp-project.atlassian.net",
