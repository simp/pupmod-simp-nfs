    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 6.4.0 < 8.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.6.0 < 8.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 4.1.0 < 5.0.0"
    },
    {
      "name": "simp/svckill",
      "version_requirement": ">= 3.6.1 < 4.0.0"
    },
    {
      "name": "simp/vox_selinux",
