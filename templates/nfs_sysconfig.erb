<% if scope.lookupvar('::nfs::mountd_nfs_v1') then -%>
MOUNTD_NFS_V1=yes
<% else -%>
MOUNTD_NFS_V1=no
<% end -%>
<% if scope.lookupvar('::nfs::mountd_nfs_v2') then -%>
MOUNTD_NFS_V2=yes
<% else -%>
MOUNTD_NFS_V2=no
<% end -%>
<% if scope.lookupvar('::nfs::mountd_nfs_v3') then -%>
MOUNTD_NFS_V3=yes
<% else -%>
MOUNTD_NFS_V3=no
<% end -%>
<% if @nfsv3 -%>
RQUOTAD="<%= scope.lookupvar('::nfs::rquotad') %>"
<% else -%>
RQUOTAD=no
<% end -%>
RQUOTAD_PORT=<%= scope.lookupvar('::nfs::rquotad_port') %>
<% if scope.function_defined(['$rpcrquotadopts']) && !@rpcrquotadopts.empty? then -%>
RPCRQUOTADOPTS="<%= @rpcrquotadopts %>"
<% end -%>
<% if scope.function_defined(['$lockd_arg']) && !@lockd_arg.empty? then -%>
LOCKD_ARG="<= lockd_arg %>"
<% end -%>
LOCKD_TCPPORT=<%= scope.lookupvar('::nfs::lockd_tcpport') %>
LOCKD_UDPPORT=<%= scope.lookupvar('::nfs::lockd_udpport') %>
<% if scope.lookupvar('::nfs::rpcnfsdargs') && !scope.lookupvar('::nfs::rpcnfsdargs').empty? then -%>
RPCNFSDARGS="<%= scope.lookupvar('::nfs::rpcnfsdargs') %>"
<% end -%>
RPCNFSDCOUNT=<%= scope.lookupvar('::nfs::rpcnfsdcount') %>
NFSD_V4_GRACE=<%= scope.lookupvar('::nfs::nfsd_v4_grace') %>
MOUNTD_PORT=<%= scope.lookupvar('::nfs::mountd_port') %>
STATD_PORT=<%= scope.lookupvar('::nfs::statd_port') %>
STATD_OUTGOING_PORT=<%= scope.lookupvar('::nfs::statd_outgoing_port') %>
<% if scope.lookupvar('::nfs::secure_nfs') then -%>
SECURE_NFS=yes
<% else -%>
SECURE_NFS=no
<% end -%>
