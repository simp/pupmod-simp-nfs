<% if scope.lookupvar('::nfs::mountd_nfs_v1') -%>
MOUNTD_NFS_V1=yes
<% else -%>
MOUNTD_NFS_V1=no
<% end -%>
<% if scope.lookupvar('::nfs::mountd_nfs_v2') -%>
MOUNTD_NFS_V2=yes
<% else -%>
MOUNTD_NFS_V2=no
<% end -%>
<% if scope.lookupvar('::nfs::mountd_nfs_v3') -%>
MOUNTD_NFS_V3=yes
<% else -%>
MOUNTD_NFS_V3=no
<% end -%>
<% if scope.lookupvar('::nfs::rpcnfsdargs') && !scope.lookupvar('::nfs::rpcnfsdargs').empty? -%>
RPCNFSDARGS="<%= scope.lookupvar('::nfs::rpcnfsdargs') %>"
<% end -%>
RPCNFSDCOUNT=<%= scope.lookupvar('::nfs::rpcnfsdcount') %>
NFSD_V4_GRACE=<%= scope.lookupvar('::nfs::nfsd_v4_grace') %>
MOUNTD_PORT=<%= scope.lookupvar('::nfs::mountd_port') %>
<% if scope.lookupvar('::nfs::secure_nfs') -%>
STATD_PORT=<%= scope.lookupvar('::nfs::statd_port') %>
STATD_OUTGOING_PORT=<%= scope.lookupvar('::nfs::statd_outgoing_port') %>
SECURE_NFS=yes
<% else -%>
SECURE_NFS=no
<% end -%>
